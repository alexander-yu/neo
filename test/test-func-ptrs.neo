var func<(string):void> f;

int foo(int i) {
    return 2 * i;
}

void print_name(string s) {
    print("My name is ");
    print(s);
}

void main() {
    print(print);
    print("\n");
    print(foo);
    print("\n");

    create array<func<(int):int>> a = {| foo, foo |};
    create array<array<func<(string):void>>> b =
        {| {| print, print_name |}, {| print_name |} |};

    print(a);
    print("\n");
    print(b[0]);
    print("\n");
    b[0][0]("Hello!\n");

    var func<(int):int> g = a[0];
    print(g(3));
    print("\n");

    f = b[1][0];
    f("Alex");
}
